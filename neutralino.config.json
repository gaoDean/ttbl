{
  "applicationId": "ttbl.timetable",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/resources/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "os.*",
	"window.*",
    "debug.log"
  ],
  "modes": {
    "window": {
      "title": "ttbl",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "alwaysOnTop": true,
      "icon": "/resources/img/appIcon.png",
      "enableInspector": true,
      "hidden": true,
      "resizable": true,
      "exitProcessOnClose": false
    }
  },
  "cli": {
    "binaryName": "ttbl",
    "resourcesPath": "/resources/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/resources/js/neutralino.js",
    "binaryVersion": "4.7.0",
    "clientVersion": "3.6.0"
  }
}
