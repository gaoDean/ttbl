{
  "applicationId": "timetable",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/app/",
  "url": "/",
  "port": 0,
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "os.*",
	"window.*",
	"storage.*",
    "debug.log"
  ],
  "modes": {
    "window": {
      "title": "ttbl",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "alwaysOnTop": false,
      "icon": "/app/img/appIcon.png",
      "enableInspector": true,
      "hidden": true,
      "resizable": true,
      "exitProcessOnClose": false
    }
  },
  "cli": {
    "binaryName": "ttbl",
    "resourcesPath": "/app/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/app/js/lib/neutralino.js",
    "binaryVersion": "4.7.0",
    "clientVersion": "3.6.0"
  }
}
